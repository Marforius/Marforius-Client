			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				RaiseFrameLevel(self);
			</OnLoad>
			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
		</Scripts>
	</Button>
	<Button name="ChannelButtonTemplate" virtual="true">
		<Size>
			<AbsDimension x="155" y="20"/>
		</Size>
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="$parentCollapsed" inherits="GameFontDisableLarge">
					<Anchors>
						<Anchor point="RIGHT">
